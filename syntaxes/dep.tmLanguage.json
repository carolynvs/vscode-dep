{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "dep",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.dep",
				"match": "\\b(required|ignored|constraint|override|metadata|name|source|branch|revision|version|prune|non-go|unused-packages|go-tests|project)\\b"
			}]
		}
	},
	"scopeName": "text.toml.dep"
}